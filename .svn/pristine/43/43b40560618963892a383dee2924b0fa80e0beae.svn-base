package com.kuaibiandang.model;

public class BusinessShow {
	/**
	 * 商家用于展示的实体类 商家名字，商家图片，商家地址，商家坐标，商家好评，商家的菜品信息查询dish表
	 */
	private String business_name;
	private String business_image_url;
	private String business_address;
	private String business_point;
	private String business_reputation;

	public String getBusiness_name() {
		return business_name;
	}

	public void setBusiness_name(String business_name) {
		this.business_name = business_name;
	}

	public String getBusiness_image_url() {
		return business_image_url;
	}

	public void setBusiness_image_url(String business_image_url) {
		this.business_image_url = business_image_url;
	}

	public String getBusiness_address() {
		return business_address;
	}

	public void setBusiness_address(String business_address) {
		this.business_address = business_address;
	}

	public String getBusiness_point() {
		return business_point;
	}

	public void setBusiness_point(String business_point) {
		this.business_point = business_point;
	}

	public String getBusiness_reputation() {
		return business_reputation;
	}

	public void setBusiness_reputation(String business_reputation) {
		this.business_reputation = business_reputation;
	}

}
