package com.kuaibiandang.model;

public class Dish {
	/*
	 * 菜品实体类
	 */
	private String dish_id;
	private String dish_name;
	private String dish_image_url;
	private float dish_price;
	private int dish_status;
	private String dish_description;
	private int dish_time; // 做菜时间
	private int dish_style;
	private String dish_discount;

	private String dish_business_id;

	public String getDish_id() {
		return dish_id;
	}

	public void setDish_id(String dish_id) {
		this.dish_id = dish_id;
	}

	public String getDish_name() {
		return dish_name;
	}

	public void setDish_name(String dish_name) {
		this.dish_name = dish_name;
	}

	public String getDish_image_url() {
		return dish_image_url;
	}

	public void setDish_image_url(String dish_image_url) {
		this.dish_image_url = dish_image_url;
	}

	public String getDish_description() {
		return dish_description;
	}

	public void setDish_description(String dish_description) {
		this.dish_description = dish_description;
	}

	public String getDish_business_id() {
		return dish_business_id;
	}

	public void setDish_business_id(String dish_business_id) {
		this.dish_business_id = dish_business_id;
	}

	public int getDish_status() {
		return dish_status;
	}

	public void setDish_status(int dish_status) {
		this.dish_status = dish_status;
	}

	public float getDish_price() {
		return dish_price;
	}

	public void setDish_price(float dish_price) {
		this.dish_price = dish_price;
	}

	public int getDish_time() {
		return dish_time;
	}

	public void setDish_time(int dish_time) {
		this.dish_time = dish_time;
	}

	public int getDish_style() {
		return dish_style;
	}

	public void setDish_style(int dish_style) {
		this.dish_style = dish_style;
	}

	public String getDish_discount() {
		return dish_discount;
	}

	public void setDish_discount(String dish_discount) {
		this.dish_discount = dish_discount;
	}
}
